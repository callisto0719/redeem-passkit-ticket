{
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/passkit/passkit-php-grpc-sdk.git"
        }
    ],
    "require": {
        "php": ">=8.2",
        "passkit/passkit-php-grpc-sdk": "1.1.70",
        "google/cloud-functions-framework": "^1.3",
        "guzzlehttp/guzzle": "^7.8"
    },
    "scripts": {
        "start-redeem-ticket": [
            "Composer\\Config::disableProcessTimeout",
            "FUNCTION_TARGET=redeemTicket php -S localhost:${PORT:-8080} vendor/google/cloud-functions-framework/router.php"
        ]
    }
}
